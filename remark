//
void setup(void)
{
	Serial.begin(9600);
	Serial.println("Uno, Relay Go Ver: 2021.05.27");

	pinMode(pinLED1, OUTPUT);
	pinMode(pinRelay1, OUTPUT);
	digitalWrite(pinRelay1, LOW);

	//keyPrint();
	Serial.println("Relay Go Ready Yet.");
}
